{
  "compilerOptions": {
    "strict": true,
    "jsx": "preserve",
    "target": "ESNext",
    "declaration": true,
    "skipLibCheck": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowArbitraryExtensions": true,
    "useUnknownInCatchVariables": true,
    "forceConsistentCasingInFileNames": true
  }
}
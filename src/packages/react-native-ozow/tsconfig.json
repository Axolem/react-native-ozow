{
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "target": "ESNext",
    "declaration": true,
    "skipLibCheck": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "useUnknownInCatchVariables": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true
  }
}